<div class="subpanel-header default-cursor">
    <div class="default-cursor">
        {{moduleLabel moduleName 'sm' class='pull-left default-cursor'}}
        <h4 class="pull-left default-cursor">{{getModuleName moduleName plural=true}}</h4>
    </div>
    <div class="subpanel-controls btn-toolbar pull-right ">
        <span class="pull-right maps-small-margin-right">
            <input type="checkbox" data-fieldname="enableModule" data-modulename="{{moduleName}}" {{#if checked}}checked{{/if}}>
        </span>
    </div>
</div>
