<div class="{{#if data.runtime}}advanced-condition border border-[--border-base] flex items-center h-[2.1875rem] rounded{{else}}advanced-condition-locked flex items-center{{/if}} mt-5 ltr:mr-4 rtl:ml-4" id="{{data.elementId}}">
    {{#unless data.runtime}}
        <i class="sicon sicon-lock ltr:ml-2 rtl:mr-2" data-widget="non-runtime-filter" rel="tooltip"></i>
    {{/unless}}
    <h5 class="ltr:ml-2 rtl:mr-2 two-rows-clamped-text {{#unless data.runtime}}disabled-advanced-condition{{/unless}}" id="{{data.tooltipId}}" rel="tooltip">{{#sanitize}}{{{data.value}}}{{/sanitize}}</h5>
</div>
