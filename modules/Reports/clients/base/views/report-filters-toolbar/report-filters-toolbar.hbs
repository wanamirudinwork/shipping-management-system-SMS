<h4 data-toggle="dashlet" class="dashlet-title report-panel-title flex-grow m-0 ml-1.5">{{str "LBL_REPORT_FILTERS" module}}</h4>
{{#if _isDetail}}
    <div class="btn-toolbar my-0 pl-0">
        <span class="dashlet-toolbar">
            <a rel="tooltip" data-bs-placement="bottom" aria-label="{{str 'LBL_ADVANCED_FILTERS' module}}" data-panelaction="toggleAdvancedFilters" class="btn btn-invisible" data-original-title="{{str 'LBL_ADVANCED_FILTERS' module}}">
                <i class="sicon sicon-list"></i>
            </a>
            {{#each meta.panels}}
                {{#each buttons}}
                    {{field ../../this}}
                {{/each}}
            {{/each}}
        </span>
    </div>
{{/if}}
