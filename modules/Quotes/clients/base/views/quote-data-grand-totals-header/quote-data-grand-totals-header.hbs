<div class="focus-within:z-40 quote-data-panel-controls border-[--border-base] ltr:border-r rtl:border-l grow-0 w-[4.5rem] z-10">
    {{#each meta.buttons}}
        {{field ../this model=../model template='list-header'}}
    {{/each}}
</div>
