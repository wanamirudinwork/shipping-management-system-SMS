<span class="ellipsis_inline pb-1.5 text-xs" data-placement="bottom" title="{{str 'LBL_AVAILABLE_MODULES' module}}">
    {{str 'LBL_AVAILABLE_MODULES' module}}
</span>
<ul class="sortable-list bg-[--background-base] border border-[--border-base] first:bg-ocean-400 h-72 ml-0 overflow-auto rounded w-48" data-name="display">
    {{#each value.display}}
        <li class="sortable-list-item ellipsis_inline py-1 px-2" data-name="{{this}}" data-placement="bottom" title="{{getModuleName this plural=true}}">
            {{getModuleName this plural=true}}
        </li>
    {{/each}}
</ul>
