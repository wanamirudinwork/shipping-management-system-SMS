<div class="ellipsis_inline relate-field-container" data-bs-placement="bottom" title="{{value}}">
    {{#if href}}
        <a class="ellipsis_inline date-name-link {{module}}" href="{{#if def.events}}javascript:void(0);{{else}}{{href}}{{/if}}"
           data-link-target="{{linkTarget}}" data-module="{{module}}" data-model-id="{{modelId}}"
           data-bs-placement="bottom" rel="tooltip" title="{{value}}"
        >
            {{#notEq serviceStartDate serviceEndDate}}
                {{serviceStartDate}} -
            {{/notEq}}
            {{serviceEndDate}}
        </a>
        {{partial 'focus-icon' this this}}
    {{else}}
        <span class="ellipsis_inline" data-bs-placement="bottom" rel="tooltip" title="{{value}}">
            {{#notEq serviceStartDate serviceEndDate}}
                {{serviceStartDate}} -
            {{/notEq}}
            {{serviceEndDate}}
        </span>
    {{/if}}
</div>

