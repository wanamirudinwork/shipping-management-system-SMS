<div class="table">
    <p class="table-row">
        <span class="table-cell pr-1.5 break-words text-right text-gray-300 max-w-24 overflow-hidden text-ellipsis text-[11px]">{{label}}</span>
        <span class="table-cell max-w-32 overflow-hidden text-ellipsis text-[11px]">{{key}}</span>
    </p>
    <p class="table-row">
        <span class="table-cell pr-1.5 break-words text-right text-gray-300 max-w-24 overflow-hidden text-ellipsis text-[11px]">{{str 'LBL_AMOUNT' 'Forecasts'}}</span>
        <span class="table-cell max-w-32 overflow-hidden text-ellipsis text-[11px]">{{y}}</span>
    </p>
    <p class="table-row">
        <span class="table-cell pr-1.5 break-words text-right text-gray-300 max-w-24 overflow-hidden text-ellipsis text-[11px]">{{str 'LBL_PERCENT' 'Forecasts'}}</span>
        <span class="table-cell max-w-32 overflow-hidden text-ellipsis text-[11px]">{{x}}</span>
    </p>
</div>
