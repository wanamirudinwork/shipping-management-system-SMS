{{processPopulateFieldDataForDV value}}

{{#if processPopulateFieldDataForDV}}
    <table class='cj-related-fieldset-detail-view table table-striped table-bordered w-[15.625rem]'>
        {{#each value}}
            {{#if id}}
                <tr>
                    <td>
                        {{#each ../../optionsList}}
                            {{#eq @key ../id}}
                                {{this}}
                            {{/eq}}
                        {{/each}}
                    </td>
                    <td>
                        {{#each ../../populateFieldsMetaData}}
                            {{#eq @key ../id}}
                                <div id="{{name}}">
                                    {{field ../../../../this.view model=../../../../this.view.model template='detail'}}
                                </div>
                            {{/eq}}
                        {{/each}}
                    </td>
                </tr>
            {{/if}}
        {{/each}}
    </table>
{{/if}}
