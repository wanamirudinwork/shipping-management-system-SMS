{{processDataForDV value}}

{{#if processDataForDV}}
    <table class='cj-related-fieldset-detail-view table table-striped table-bordered w-[15.625rem]'>
        {{#each value}}
            {{#notEq this.id ''}}
                <tr>
                    <td>
                        {{getMainDropDownValue this.id ../../../this}}
                        
                    </td>
                    <td><ul>{{#if ../formattedRname}} {{setFormattedRnameArray ../formattedRname this ../../../this}} {{#each formattedRnameArray}} <li>{{this}}</li>{{/each}} {{/if}}</ul></td>
                </tr>
            {{/notEq}}
        {{/each}}
    </table>
{{/if}}
