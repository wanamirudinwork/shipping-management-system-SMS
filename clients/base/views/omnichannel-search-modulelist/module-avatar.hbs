{{#each icon}}
    {{#if module}}
        {{moduleLabel module 'sm' class="align-middle mr-0.5" title=(getModuleName module plural=true)}}
    {{else}}
        {{#if multipleLabel}}
            <span class="label non-module-label"
                  aria-label="{{str 'LBL_GLOBAL_SEARCH_MODULE_FILTER'}}" title="{{multipleLabel}}">
                {{multipleLabel}}
            </span>
        {{else}}
            <span class="label non-module-label" aria-label="{{str 'LBL_GLOBAL_SEARCH_MODULE_FILTER'}}" title="{{str 'LBL_MODULE_ALL'}}">{{str 'LBL_MODULE_ALL'}}</span>
        {{/if}}
    {{/if}}
{{/each}}
