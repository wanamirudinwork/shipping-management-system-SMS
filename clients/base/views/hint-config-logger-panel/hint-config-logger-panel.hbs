<div class="accordion-heading" data-bs-toggle="collapse" data-bs-target="[data-container=loggerOptions]"
    aria-expanded="false" aria-controls="[data-container=loggerOptions]">
    <a id='{{name}}Title' class="accordion-toggle"></a>
</div>
<div class="in bs-collapse collapse show" data-container="loggerOptions">
    <div class="accordion-inner">
        {{#each meta.panels}}
            {{#each fields}}
                <label for="{{name}}">{{str label 'Administration'}}</label>
                {{field ../../this model=../../model}}
            {{/each}}
        {{/each}}
    </div>
</div>
